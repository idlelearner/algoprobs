package code.ds;

/**
 * Implementation of stacks using Linked List
 * 
 * @author Dhass
 * 
 */
public class StacksLL<T> {
	class Node<T> {
		T item;
		Node<T> next;
	}
	
	private Node<T> first = null;
	
	public boolean isEmpty(){
		return first == null;
	}
	
	public void push(T item){
		Node<T> newElmt = new Node<>();
	}
}
