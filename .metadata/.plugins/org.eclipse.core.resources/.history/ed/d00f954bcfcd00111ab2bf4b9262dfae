package code;

public class Median {
	public static void main(String args[]) {

		// helpers for input/output

		Scanner in = new Scanner(System.in);

		int N;
		N = in.nextInt();

		String s[] = new String[N];
		int x[] = new int[N];

		for (int i = 0; i < N; i++) {
			s[i] = in.next();
			x[i] = in.nextInt();
		}

		int result = 0;
		System.out.println(result);

	}
}
